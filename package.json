{
  "name": "@momsfriendlydevco/docgen",
  "version": "0.0.0",
  "description": "Documentation generator for JSDoc + JSDoc-to-OpenAPI",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MomsFriendlyDevCo/docgen.git"
  },
  "keywords": [
    "jsdoc",
    "openapi",
    "documentation",
    "docs"
  ],
  "author": "Matt Carter <m@ttcarter.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MomsFriendlyDevCo/docgen/issues"
  },
  "homepage": "https://github.com/MomsFriendlyDevCo/docgen#readme",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.1",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-transform-destructuring": "^7.13.0",
    "@babel/preset-env": "^7.12.16",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "chai": "^4.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "mocha": "^8.3.0",
    "webpack": "^5.22.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "lodash": "^4.17.20"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "fsharp"
        }
      ],
      "@babel/plugin-proposal-throw-expressions"
    ]
  }
}
